                            <th class="min-w-100px">Hospital</th>
                            <th class="min-w-100px">Print Name</th>
                            <th class="min-w-100px">Print Line - 1</th>
                            <th class="min-w-100px">Print Line - 2</th>
                            <th class="min-w-100px">Print Line - 3</th>
                            <th class="min-w-100px">Footer Name</th>
                            <th class="min-w-200px">Report Header Name</th>
                            <th class="min-w-100px">Modified</th>
                            <th class="text-center">Action</th>
                        </tr>
                    </thead>
                    <tbody class="fw-semibold text-dark-600 font-weight-normal">
                        @foreach(MST_HospitalModel dr in ViewBag.HospitalList)
                        {
                            <tr>
                                <td>@dr.Hospital</td>
                                <td>@dr.PrintName</td>
                                <td>@dr.PrintLine1</td>
                                <td>@dr.PrintLine2</td>
                                <td>@dr.PrintLine3</td>
                                <td>@dr.FooterName</td>
                                <td>@dr.ReportHeaderName</td>
                                <td>@Convert.ToDateTime(dr.Modified).ToString("dd-MM-yyyy")</td>
                                <td class="text-center">
                                    <a class="text-decoration-none text-primary edit-button"
                                       data-bs-toggle="tooltip"
                                       data-bs-placement="left" data-bs-title="Edit"
                                       asp-area="MST_Hospital" asp-controller="MST_Hospital" asp-action="Add" asp-route-HospitalID="@CommonFunctions.EncryptBase64(dr.HospitalID.ToString())">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="20"
                                             height="20" fill="currentColor"
                                             class="bi bi-pencil-square" viewBox="0 0 16 16">
