@model GNForm3C_.Areas.MST_FinYear.Models.MST_FinYearModel
<form method="post">
    <div class="d-flex row">

        <!--begin::Input group-->
        <div class="row mb-8">
            <!--begin::Col-->
            <div class="col-6 col-md-6 fv-row">
                <!--begin::Input-->
                <input type="search" class="form-control form-control-solid"
                       name="FinYearName" asp-for="FinYearName" placeholder="Enter Fin Year Name"
                       autocomplete="off" />
                <!--end::Input-->
            </div>
            <!--end::Col-->
        </div>
        <!--end::Input group-->

        <div class="col-lg-4">
            <button type="submit" class="btn btn-primary"
                    style="margin-right:15px;" asp-area="MST_FinYear" asp-controller="MST_FinYear" asp-action="Index">
                <span class="indicator-label">Search</span>
            </button>
            <a type="submit" class="btn btn-light me-3"
               asp-area="MST_FinYear" asp-area="MST_FinYear" asp-controller="MST_FinYear" asp-action="Index">
                Clear
            </a>
        </div>
    </div>
</form>